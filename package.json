{
  "name": "discord-bot",
  "version": "1.0.0",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "fix": "biome check --write --unsafe",
    "check": "biome check",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@types/he": "^1.2.3",
    "cron": "^4.1.0",
    "discord.js": "14.24.2",
    "dotenv": "^17.0.0",
    "googleapis": "^164.0.0",
    "he": "^1.2.0",
    "mysql2": "^3.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.3",
    "@types/node": "^24.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*": [
      "biome check --write --unsafe --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
